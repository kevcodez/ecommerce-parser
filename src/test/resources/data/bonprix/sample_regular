<script>

    getWtValueByMetaTag = function(name) {
        var meta, _i, _len, _ref;
        _ref = document.getElementsByTagName("meta");
        for (_i = 0, _len = _ref.length; _i < _len; _i++) {
            meta = _ref[_i];
            if (meta.getAttribute("name") === ("wt:" + name)) {
                return meta.getAttribute("content");
            }
        }
    };

    window.webtrendsAsyncInit = function () {
        var dcs = new Webtrends.dcs().init({
            dcsid: getWtValueByMetaTag("dcsid"),
            domain: "statse.webtrendslive.com",
            timezone: 1,
            i18n: true,
            fpcdom: getWtValueByMetaTag("fpcdom"),
            plugins: {otto: {src: "https://static.bonprixsecure.com/resource/20180104_072538/desktop/scripts/libs/webtrends6/plugin.js"} }
        });
        dcs.track({
            transform: function(_tag, options) {
                _tag.WT.z_ecid = "EC0201";
                _tag.DCS.dcsipa = "1";

                // Add EMail
                                // AddUID
                                            }
        });
    };
</script>
