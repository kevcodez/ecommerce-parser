    requirejs.onError = function (err) {
        console.log(err.requireType);
        if (err.requireType === 'timeout') {
            console.log('modules: ' + err.requireModules);
        }
        else if (err.requireType === 'mismatch') {
            console.log("requireJs mismatch error caught!");
        } else {
            throw err;
        }
    };

     var WindowMeasurements = {
        viewportHeight: function() {
            return window.innerHeight ||
                   document.documentElement.clientHeight ||
                   document.body.clientHeight || 0;
        },

        scrollY:function() {
            return window.pageYOffset ||
                   document.body.scrollTop ||
                   document.documentElement.scrollTop || 0;
        },

        documentHeight: function() {
            return Math.max(
                document.body.scrollHeight || 0,
                document.documentElement.scrollHeight || 0,
                document.body.offsetHeight || 0,
                document.documentElement.offsetHeight || 0,
                document.body.clientHeight || 0,
                document.documentElement.clientHeight || 0
            );
        },
        scrollYPercentage: function() {
            return parseInt((
                (this.scrollY() + this.viewportHeight()) / this.documentHeight()
            ) * 10000)/100;
        }

    }


    /**
     * Get current vertical scroll percentage
     */

    window.controllerCount = -1;
    window.controllerLoadTimestamp = new Date().getTime();
    window.moduleCount = -1;
    window.controllerLoadTime = -1;

    require(['domReady'],function(ready){
        ready(function(){
            window.documentHeight = WindowMeasurements.documentHeight();
            window.scrolledHeight = WindowMeasurements.scrollY() + WindowMeasurements.viewportHeight();
            window.scrollPercentage = WindowMeasurements.scrollYPercentage();

            if(!window.addEventListener){    // IE8 compatibility issue
                window.attachEvent("onscroll",function(){
                    var scrolledHeight = WindowMeasurements.scrollY() + WindowMeasurements.viewportHeight();
                    if(window.scrolledHeight < scrolledHeight) {
                        window.scrolledHeight = scrolledHeight;
                        window.scrollPercentage = WindowMeasurements.scrollYPercentage();
                        window.documentHeight = WindowMeasurements.documentHeight();
                    }
                });
                window.attachEvent("onunload",function(event){
                    var paramString = convertMapToParamString({
                        scrollPercentage:window.scrollPercentage,
                        scrolledHeight:window.scrolledHeight,
                        documentHeight:window.documentHeight,
                        url: location.href
                    })
                    if (false) {
                        navigator.sendBeacon("https://www.bonprix.de/log/javascript/performance/scrolling/?"+paramString,{})
                    }
                });
            }
            else {
                window.addEventListener("scroll",function(){
                    var scrolledHeight = WindowMeasurements.scrollY() + WindowMeasurements.viewportHeight();
                    if(window.scrolledHeight < scrolledHeight) {
                        window.scrolledHeight = scrolledHeight;
                        window.scrollPercentage = WindowMeasurements.scrollYPercentage();
                        window.documentHeight = WindowMeasurements.documentHeight();
                    }
                }, false);
                window.addEventListener("unload",function(event){
                    var paramString = convertMapToParamString({
                        scrollPercentage:window.scrollPercentage,
                        scrolledHeight:window.scrolledHeight,
                        documentHeight:window.documentHeight,
                        url: location.href
                    })
                    if (false) {
                        navigator.sendBeacon("https://www.bonprix.de/log/javascript/performance/scrolling/?"+paramString,{})
                    }
                }, false);
            }
        });
    })

    require(['inline/controller'],function(controller){
        controller.ready(function(){
            window.moduleCount = document.querySelectorAll("[data-requiremodule]").length;
            console.log(window.moduleCount + " AMD Modules loaded!");
            var paramString = convertMapToParamString({
                    moduleCount:window.moduleCount,
                    controllerCount:window.controllerCount,
                    msUntilControllerReady:window.controllerLoadTime,
                    url: location.href,
            })
            if (false) {
                navigator.sendBeacon("https://www.bonprix.de/log/javascript/performance/bootstrap/?"+paramString,{})
            }
        });
    });

    initErrorLogging("http://static.bonprixsecure.com/resource","https://static.bonprixsecure.com/resource","https://www.bonprix.de/log/javascript/error/");
    requireReady = function(callback){
        require(['jquery','common/logger'],function(jQuery,logger){
            callback(jQuery,logger);
        });
    }
    if(window.requireReadyCallbacks.length > 0) {
        require(['jquery', 'common/logger'], function (jQuery, logger) {
            for (var i = 0; i < window.requireReadyCallbacks.length; i++) {
                var method = window.requireReadyCallbacks[i];
                if(typeof method == "function"){
                    method(jQuery, logger);
                }
            }
        });
    }
</script>











        <script type="text/javascript">
        function getCookie(name){
            var keyValue = document.cookie.match('(^|;) ?' + name + '=([^;]*)(;|$)');
            return keyValue ? decodeURIComponent(keyValue[2]) : null;
        }
        var owaParams = {};
