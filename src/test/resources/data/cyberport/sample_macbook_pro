            <script type="text/javascript" no_defer="">
    /* <![CDATA[ */
    function getVisitFrequencyGTM()
    {
        var result = "unbekannt oder erstbesuch";
        var lastVisit = parseInt(getCookie("CP_GTM_VF"));
        var curDate = new Date();
        var curTime = curDate.getTime();

        if (typeof lastVisit === 'number' && lastVisit % 1 === 0) {
            var seconds = (curTime - lastVisit) / 1000;
            var hours = Math.floor(seconds / 3600);

            if (hours <= 4) {
                result = "kleiner-gleich_4_stunden";
            } else if (hours > 4 && hours <= 24) {
                result = "ueber_4_unter_24_stunden";
            } else if (hours > 24 && hours <= 720) {
                result = "ueber_1_tag_bis_1_monat";
            } else if (hours > 720 && hours <= 8760) {
                result = "ueber_1_monat_bis_1_jahr";
            } else {
                result = "ueber_1_jahr";
            }
        }

        // Cookie-Lifetime von 10 Jahren
        setTrackingCookie("CP_GTM_VF", curTime, 5256000);

        return result;
    }

    function setTrackingCookie(label, val, minutes)
    {
        var tExpDate = new Date();
        tExpDate.setTime(tExpDate.getTime() + (minutes*60*1000));
        document.cookie = label + "=" + escape(val) + ((minutes === null) ? "" : ";expires=" + tExpDate.toGMTString()) + "; path=/";
    }

    config.set("wt_productCategory", "Apple und Zubehör > Apple MacBook Pro");

    var wt_counter = getCookie("CP_GTM_COUNTER");
    if (typeof (wt_counter) !== "undefined") {
        wt_counter++;
    } else {
        wt_counter = 1;
    }

    setTrackingCookie("CP_GTM_COUNTER", wt_counter, 30);

    // letzten Besuch ueber JS holen wegen Varnish (nur bei erstem Aufruf erforderlich)
    wt_visitFrequency = "";
    if (wt_counter === 1) {
        wt_visitFrequency = getVisitFrequencyGTM();
    }


