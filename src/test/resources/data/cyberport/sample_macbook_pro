<script data-name="reco-engine" type="text/javascript" no_defer="">

//watcher-events fuer emarsys bei reco-engine anders konfigurieren
//achtung - standard events werden hier ueberschrieben damit reco und tracking in die queue geschrieben werden koennen
var emarsysQueueWatcherEvents = emarsysQueueWatcherEvents || ['tracking', 'reco'];

jQuery(function() {
    // widget-template-raw-data
    var widgetTemplateData = ["{{ if (SC.page.products.length) { }}",
"{{ for (var i=0; i < SC.page.products.length; i++) { }}",
"{{ var p = SC.page.products[i]; }}",
"<li>",
"<div class=\"prodbox\" data-scarabitem=\"{{= p.item}}\">",
"<a data-i=\":{{= i}}:\" data-lazyImage=\"{{= p.image}}\" data-itemnumber=\"{{= p.item}}\" href=\"{{= p.link}}\">",
"<img id=\"productslide{{= p.id}}\" class=\"gtmClicktrackProduct product infobig\" width=\"120\" height=\"120\" data-articlenumber=\"{{= p.item}}\" data-context=\"recommendations\" data-element=\"productImage\" alt=\"{{= p.name}}\" src=\"{{= p.image}}\">",
"<\/a>",
"<div class=\"prodglider-titel\" data-itemnumber=\"{{= p.item}}\" >",
"<p>",
"<a data-i=\":{{= i}}:\" class=\"gtmClicktrackProduct infobig2\" data-element=\"productTitle\" data-context=\"recommendations\" data-articlenumber=\"{{= p.item}}\" data-sourcetemplate=\"\" href=\"{{= p.link}}\" id=\"productslide2{{= p.id}}\"><span class=\"colorSpezial fl r4\">",
"{{ if (p.ispricehit) { }}<span class=\"colorSpezial fl r4\">%Sale<\/span>{{ } }}",
"<\/span>{{= p.title}}<\/a>",
"<\/p>",
"<\/div>",
"<div class=\"itemcostinfo\" costinfotype=\"flat\" itemid=\"{{= p.id}}\">{{= p.costinfo}}<\/div>",
"<\/div>",
"<\/li>",
"{{ } }}",
"{{ } }}"];
    var myWidgetContent = '';
    for (var i=0; i<widgetTemplateData.length; i++) {
        myWidgetContent = myWidgetContent + widgetTemplateData[i];
    }

    //api-script
    var emarsysRecoDomain =
        ('https:' === document.location.protocol ? 'https://recommender' : 'http://cdn') +
        '.scarabresearch.com/js/155DBCF72209E02A/scarab-v2.js';

    //reco-engine-helper
    var emarsysReco = new Recommendation.Emarsys(emarsysRecoDomain, 'scarab-js-api', {
        itemcount               : 16,
        containerid             : 'listWrapper',
        template                : '' + myWidgetContent,
        sliderSelector          : '#altProd-container .slider ul',
        tooltipSelector         : 'altProd-container',
        itemSelector            : '.prodbox',
        onAfterGliderItemUpdate : function(rdataitem) {
            var tooltipConfig = {
                sku        : rdataitem.artid,
                itemNumber : rdataitem.artnr,
                ajax       : {
                    url     : '/newajax/catalog/item/' + rdataitem.artnr + '/tooltip',
                    params  : {
                        itemUrlPath  : rdataitem.sweet_link,
                        fromTemplate : 'recommend'
                    }
                }
            };
            config.set('productslide' + rdataitem.artid, tooltipConfig);
            config.set('productslide2' + rdataitem.artid, tooltipConfig);

        }
    });

    //start zum erstellen der reco-engine-queue-eintraege
    emarsysReco.pushMessage('RELATED');
});
</script></div>

